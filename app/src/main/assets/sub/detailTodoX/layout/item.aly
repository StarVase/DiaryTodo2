{--设置项(图片,标题,开关)
  LinearLayout;
  gravity="center";
  layout_width="fill";
  paddingLeft=math.dp2int(8);
  paddingRight=math.dp2int(8);
  paddingTop=math.dp2int(8);
  paddingBottom=math.dp2int(8);

  layout_height="wrap";
  {
    AppCompatCheckBox;
    id="status";
    Focusable=false;
    clickable=false;
    layout_marginRight="16dp";
    layout_marginLeft="16dp";
    --style="@style/Widget.AppCompat.CompoundButton.RadioButton"
  };
  {
    AppCompatTextView;
    id="title";
    textSize="16sp";
    --gravity="center";
    --layout_height="fill";
    layout_weight="1";
    layout_marginLeft="16dp";
    layout_marginRight="16dp";
    textColor=textColor;
    singleLine="true";
  };

};