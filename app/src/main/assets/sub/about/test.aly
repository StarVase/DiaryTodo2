{
  CoordinatorLayout;
  layout_width="fill";
  layout_height="fill";
  id="linear";
  fitsSystemWindows=true,
  {
    AppBarLayout;
    layout_width="fill";
    layout_height="205dp";
    id="appBarLayout",
    fitsSystemWindows=true,
    {
      SubtitleCollapsingToolbarLayout;
      layout_height="fill";
      layout_width="fill";
      id="collapsingToolbarLayout",
      scrimAnimationDuration=400,
      fitsSystemWindows=true,
      {
        FrameLayout,
        layout_width="fill",
        layout_height="305dp",
        id="imageFrame",
        fitsSystemWindows=true,

        {
          AppCompatImageView,
          layout_width="fill",
          layout_height="305dp";
          scaleType="centerCrop";
          id="mainAppCompatImageView",

        },
        {
          AppCompatImageView,
          layout_width="fill",
          layout_height="305dp";
          scaleType="centerCrop";
          alpha=0,
          id="subAppCompatImageView",
        },
      },
      {
        Toolbar,
        layout_height="56dp";
        layout_width="fill";
        id="toolBar",
      },
    };
  };
  {
    NestedScrollView,
    id="nestedScrollView",
    --layout_behavior="@string/appbar_scrolling_view_behavior",

  }
};
